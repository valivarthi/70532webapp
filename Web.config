<configuration>
  <connectionStrings>
    <!--TO DO-->
    <!--In the following connectionString replace data-source, initial-catalog, user-id and password for the Azure SQL Connection -->
    <!--OR-->
    <!--Replace the entire connectionString with Azure SQL connectionString from App.config of QuickServiceDAL -->
    <add name="QuickServiceDB001Context" connectionString="metadata=res://*/QuickServiceModel.csdl|res://*/QuickServiceModel.ssdl|res://*/QuickServiceModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=data-source;initial catalog=initial-catalog;user id=user-id;password=password;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient"/>
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <!--In the following replace account-name and account-key for the Azure Storage Connection -->
    <!--OR-->
    <!--Replace the entire StorageConnectionString with Azure Storage Connection from App.config of QuickServiceDAL -->
    <add key="StorageConnectionString" value="DefaultEndpointsProtocol=https;AccountName=account-name;AccountKey=account-key"/>
    <!--In the following replace api-endpoint-url with the hosted QuickServiceWebAPI url  -->
    <add key="ServiceUrl" value="api-endpoint-url"/>
  </appSettings>
